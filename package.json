{
  "name": "watchapi-client",
  "displayName": "WatchAPI - REST Client & API Testing",
  "description": "REST API client for testing and monitoring endpoints. Auto-import from Next.js, NestJS & tRPC. Alternative to Postman and Insomnia.",
  "version": "0.2.1",
  "icon": "assets/marketplace-logo.png",
  "publisher": "watchapi",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Testing",
    "Debuggers"
  ],
  "keywords": [
    "rest",
    "api",
    "http",
    "rest-client",
    "api-client",
    "http-client",
    "api-testing",
    "postman",
    "insomnia",
    "thunder-client",
    "request",
    "endpoint",
    "nextjs",
    "nestjs",
    "trpc",
    "swagger",
    "openapi",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/watchapi/watchapi-vscode"
  },
  "bugs": {
    "url": "https://github.com/watchapi/watchapi-vscode/issues"
  },
  "homepage": "https://github.com/watchapi/watchapi-vscode#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "watchapi.deleteEndpoint",
        "key": "delete",
        "mac": "cmd+backspace",
        "when": "view == watchapi.collections && viewItem == endpoint"
      },
      {
        "command": "watchapi.deleteCollection",
        "key": "delete",
        "mac": "cmd+backspace",
        "when": "view == watchapi.collections && viewItem == collection"
      }
    ],
    "uriHandlers": [
      {
        "id": "watchapi.watchapi-client"
      }
    ],
    "commands": [
      {
        "command": "watchapi.refresh",
        "title": "WatchAPI: Refresh Collections",
        "icon": "$(refresh)"
      },
      {
        "command": "watchapi.focus",
        "title": "WatchAPI: Focus Collection View"
      },
      {
        "command": "watchapi.openDashboard",
        "title": "WatchAPI: Open Dashboard",
        "icon": "$(globe)"
      },
      {
        "command": "watchapi.addCollection",
        "title": "WatchAPI: Add Collection",
        "icon": "$(add)"
      },
      {
        "command": "watchapi.deleteCollection",
        "title": "Delete",
        "icon": "$(bin)",
        "enablement": "view == watchapi.collections"
      },
      {
        "command": "watchapi.addEndpoint",
        "title": "Add Endpoint",
        "icon": "$(add)",
        "enablement": "view == watchapi.collections"
      },
      {
        "command": "watchapi.uploadEndpoints",
        "title": "WatchAPI: Pull from Code",
        "icon": "$(arrow-circle-down)"
      },
      {
        "command": "watchapi.deleteEndpoint",
        "title": "Delete",
        "enablement": "view == watchapi.collections"
      },
      {
        "command": "watchapi.login",
        "title": "Login",
        "icon": "$(account)"
      },
      {
        "command": "watchapi.openSettings",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "watchapi.export",
        "title": "Export",
        "icon": "$(export)"
      },
      {
        "command": "watchapi.logout",
        "title": "Logout",
        "icon": "$(sign-out)"
      },
      {
        "command": "watchapi.switchOrganization",
        "title": "WatchAPI: Switch Organization",
        "icon": "$(organization)"
      },
      {
        "command": "watchapi.findEndpoint",
        "title": "Search Endpoints",
        "icon": "$(search)"
      },
      {
        "command": "watchapi.uploadEndpoints",
        "title": "WatchAPI: Pull from Code",
        "icon": "$(arrow-circle-down)",
        "when": "watchapi.canUpload"
      }
    ],
    "viewsWelcome": [
      {
        "view": "watchapi.collections",
        "contents": "No collections yet.\n\n"
      },
      {
        "view": "watchapi.collections",
        "contents": "[$(arrow-circle-down) Pull from Code](command:watchapi.uploadEndpoints)",
        "when": "watchapi.canUpload"
      },
      {
        "view": "watchapi.collections",
        "contents": "[$(add) New Collection](command:watchapi.addCollection)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "watchapi.findEndpoint",
          "when": "view == watchapi.collections",
          "group": "navigation@0"
        },
        {
          "command": "watchapi.login",
          "when": "view == watchapi.collections && !watchapi.loggedIn",
          "group": "menu@1"
        },
        {
          "command": "watchapi.openSettings",
          "when": "view == watchapi.collections",
          "group": "menu@2"
        },
        {
          "command": "watchapi.export",
          "when": "view == watchapi.collections",
          "group": "menu@2"
        },
        {
          "command": "watchapi.logout",
          "when": "view == watchapi.collections && watchapi.loggedIn",
          "group": "menu@3"
        },
        {
          "command": "watchapi.switchOrganization",
          "when": "view == watchapi.collections && watchapi.loggedIn",
          "group": "menu@4"
        },
        {
          "command": "watchapi.addCollection",
          "when": "view == watchapi.collections",
          "group": "navigation@1"
        },
        {
          "command": "watchapi.uploadEndpoints",
          "when": "view == watchapi.collections && watchapi.canUpload",
          "group": "navigation@2"
        },
        {
          "command": "watchapi.refresh",
          "when": "view == watchapi.collections",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "watchapi.deleteCollection",
          "when": "view == watchapi.collections && (viewItem == collection || !viewItem)",
          "group": "navigation@99"
        },
        {
          "command": "watchapi.addEndpoint",
          "when": "view == watchapi.collections && viewItem == collection",
          "group": "inline"
        },
        {
          "command": "watchapi.deleteEndpoint",
          "when": "view == watchapi.collections && (viewItem == endpoint || !viewItem)",
          "group": "navigation@99"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "watchapi",
          "title": "WatchAPI Client",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "watchapi": [
        {
          "id": "watchapi.collections",
          "name": ""
        }
      ]
    },
    "configuration": {
      "title": "WatchAPI",
      "properties": {
        "watchapi.apiUrl": {
          "type": "string",
          "default": "https://watchapi.dev",
          "markdownDescription": "WatchAPI backend URL. Change this if you're running a self-hosted instance.",
          "order": 1
        },
        "watchapi.dashboardUrl": {
          "type": "string",
          "default": "https://watchapi.dev",
          "markdownDescription": "WatchAPI dashboard URL for opening in browser.",
          "order": 2
        },
        "watchapi.includeAuthorizationHeader": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Automatically add an empty `Authorization` header to new endpoints. Useful for APIs that require authentication.",
          "order": 3
        }
      }
    }
  },
  "scripts": {
    "vscode:package": "pnpm run package && pnpm exec vsce package --no-dependencies",
    "vscode:publish": "pnpm exec vsce publish --no-dependencies",
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@trpc/client": "^11.0.0",
    "@vscode/test-cli": "^0.0.12",
    "flat": "^6.0.1",
    "ts-morph": "^27.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "22.x",
    "@types/vscode": "^1.90.0",
    "@vitest/ui": "^2.0.0",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "ovsx": "^0.10.0",
    "ts-loader": "^9.5.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^2.0.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}