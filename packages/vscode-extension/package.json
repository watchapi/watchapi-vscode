{
    "name": "watchapi-client",
    "displayName": "WatchAPI - Code-Synced API Client for Next.js, NestJS & tRPC",
    "description": "Auto-sync API requests directly from your backend code. Built for Next.js, NestJS, and tRPC - no manual setup, no drift.",
    "version": "0.4.3",
    "icon": "assets/marketplace-logo.png",
    "publisher": "watchapi",
    "engines": {
        "vscode": "^1.90.0"
    },
    "sponsor": {
        "url": "https://github.com/sponsors/arseniyx"
    },
    "categories": [
        "Testing",
        "Debuggers"
    ],
    "keywords": [
        "api",
        "http",
        "api-sync",
        "code-synced",
        "schema-sync",
        "endpoint-sync",
        "api-from-code",
        "source-of-truth",
        "nestjs",
        "trpc",
        "nextjs",
        "payload",
        "backend",
        "typescript",
        "api-client",
        "request"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/watchapi/watchapi",
        "directory": "packages/vscode-extension"
    },
    "bugs": {
        "url": "https://github.com/watchapi/watchapi/issues"
    },
    "homepage": "https://github.com/watchapi/watchapi/tree/main/packages/vscode-extension#readme",
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "http",
                "extensions": [
                    ".http",
                    ".rest"
                ],
                "aliases": [
                    "HTTP",
                    "http"
                ]
            }
        ],
        "grammars": [
            {
                "language": "http",
                "scopeName": "source.http",
                "path": "./syntaxes/http.tmLanguage.json"
            }
        ],
        "keybindings": [
            {
                "command": "watchapi.deleteEndpoint",
                "key": "delete",
                "mac": "cmd+backspace",
                "when": "view =~ /^watchapi\\.collections/ && viewItem == endpoint"
            },
            {
                "command": "watchapi.deleteCollection",
                "key": "delete",
                "mac": "cmd+backspace",
                "when": "view =~ /^watchapi\\.collections/ && viewItem == collection"
            }
        ],
        "uriHandlers": [
            {
                "id": "watchapi.watchapi-client"
            }
        ],
        "commands": [
            {
                "command": "watchapi.refresh",
                "title": "WatchAPI: Refresh Collections",
                "icon": "$(refresh)"
            },
            {
                "command": "watchapi.focus",
                "title": "WatchAPI: Focus Collection View"
            },
            {
                "command": "watchapi.openDashboard",
                "title": "WatchAPI: Open Dashboard",
                "icon": "$(globe)"
            },
            {
                "command": "watchapi.addCollection",
                "title": "WatchAPI: Add Collection",
                "icon": "$(add)"
            },
            {
                "command": "watchapi.deleteCollection",
                "title": "Delete",
                "icon": "$(bin)",
                "enablement": "view =~ /^watchapi\\.collections/"
            },
            {
                "command": "watchapi.addEndpoint",
                "title": "Add Endpoint",
                "icon": "$(add)",
                "enablement": "view =~ /^watchapi\\.collections/"
            },
            {
                "command": "watchapi.deleteEndpoint",
                "title": "Delete",
                "enablement": "view =~ /^watchapi\\.collections/"
            },
            {
                "command": "watchapi.login",
                "title": "Login",
                "icon": "$(account)"
            },
            {
                "command": "watchapi.openSettings",
                "title": "Settings",
                "icon": "$(gear)"
            },
            {
                "command": "watchapi.export",
                "title": "Export",
                "icon": "$(export)"
            },
            {
                "command": "watchapi.logout",
                "title": "Logout",
                "icon": "$(sign-out)"
            },
            {
                "command": "watchapi.switchOrganization",
                "title": "WatchAPI: Switch Organization",
                "icon": "$(organization)"
            },
            {
                "command": "watchapi.findEndpoint",
                "title": "Search Endpoints",
                "icon": "$(search)"
            },
            {
                "command": "watchapi.syncFromCode",
                "title": "WatchAPI: Sync from Code",
                "icon": "$(sync)"
            },
            {
                "command": "watchapi.configureSync",
                "title": "Configure Sync",
                "icon": "$(gear)"
            }
        ],
        "viewsWelcome": [
            {
                "view": "watchapi.collections",
                "contents": "No collections yet.\n\n"
            },
            {
                "view": "watchapi.collections",
                "contents": "[$(add) New Collection](command:watchapi.addCollection)"
            },
            {
                "view": "watchapi.collectionsExplorer",
                "contents": "No collections yet.\n\n"
            },
            {
                "view": "watchapi.collectionsExplorer",
                "contents": "[$(add) New Collection](command:watchapi.addCollection)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "watchapi.findEndpoint",
                    "when": "view =~ /^watchapi\\.collections/",
                    "group": "navigation@0"
                },
                {
                    "command": "watchapi.login",
                    "when": "view =~ /^watchapi\\.collections/ && !watchapi.loggedIn",
                    "group": "menu@1"
                },
                {
                    "command": "watchapi.openSettings",
                    "when": "view =~ /^watchapi\\.collections/",
                    "group": "menu@2"
                },
                {
                    "command": "watchapi.export",
                    "when": "view =~ /^watchapi\\.collections/",
                    "group": "menu@2"
                },
                {
                    "command": "watchapi.logout",
                    "when": "view =~ /^watchapi\\.collections/ && watchapi.loggedIn",
                    "group": "menu@3"
                },
                {
                    "command": "watchapi.switchOrganization",
                    "when": "view =~ /^watchapi\\.collections/ && watchapi.loggedIn",
                    "group": "menu@4"
                },
                {
                    "command": "watchapi.addCollection",
                    "when": "view =~ /^watchapi\\.collections/",
                    "group": "navigation@1"
                },
                {
                    "command": "watchapi.syncFromCode",
                    "when": "view =~ /^watchapi\\.collections/ && watchapi.canUpload",
                    "group": "navigation@2"
                },
                {
                    "command": "watchapi.configureSync",
                    "when": "view =~ /^watchapi\\.collections/ && watchapi.canUpload",
                    "group": "menu@5"
                },
                {
                    "command": "watchapi.refresh",
                    "when": "view =~ /^watchapi\\.collections/",
                    "group": "navigation@3"
                }
            ],
            "view/item/context": [
                {
                    "command": "watchapi.deleteCollection",
                    "when": "view =~ /^watchapi\\.collections/ && (viewItem == collection || !viewItem)",
                    "group": "navigation@99"
                },
                {
                    "command": "watchapi.addEndpoint",
                    "when": "view =~ /^watchapi\\.collections/ && viewItem == collection",
                    "group": "inline"
                },
                {
                    "command": "watchapi.deleteEndpoint",
                    "when": "view =~ /^watchapi\\.collections/ && (viewItem == endpoint || !viewItem)",
                    "group": "navigation@99"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "watchapi",
                    "title": "WatchAPI Client",
                    "icon": "assets/icon.svg"
                }
            ]
        },
        "views": {
            "watchapi": [
                {
                    "id": "watchapi.collections",
                    "name": ""
                }
            ],
            "explorer": [
                {
                    "id": "watchapi.collectionsExplorer",
                    "name": "WatchAPI"
                }
            ]
        },
        "configuration": {
            "title": "WatchAPI",
            "properties": {
                "watchapi.apiUrl": {
                    "type": "string",
                    "default": "https://watchapi.dev",
                    "markdownDescription": "WatchAPI backend URL. Change this if you're running a self-hosted instance.",
                    "order": 1
                },
                "watchapi.dashboardUrl": {
                    "type": "string",
                    "default": "https://watchapi.dev",
                    "markdownDescription": "WatchAPI dashboard URL for opening in browser.",
                    "order": 2
                },
                "watchapi.includeAuthorizationHeader": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Automatically add an empty `Authorization` header to new endpoints. Useful for APIs that require authentication.",
                    "order": 3
                },
                "watchapi.includeDefaultSetDirective": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Automatically add `@set authToken` directive to login/register endpoints to capture authentication tokens from responses.",
                    "order": 4
                },
                "watchapi.autoSync.enabled": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Automatically sync endpoints when source files (routes, controllers) are saved.",
                    "order": 4
                }
            }
        },
        "configurationDefaults": {
            "editor.tokenColorCustomizations": {
                "textMateRules": [
                    {
                        "scope": "entity.name.function.http",
                        "settings": {
                            "foreground": "#4EC9B0"
                        }
                    },
                    {
                        "scope": "constant.language.http",
                        "settings": {
                            "foreground": "#569CD6"
                        }
                    },
                    {
                        "scope": "variable.language.http",
                        "settings": {
                            "foreground": "#CE9178"
                        }
                    },
                    {
                        "scope": "invalid.illegal.http",
                        "settings": {
                            "foreground": "#F44747"
                        }
                    }
                ]
            }
        }
    },
    "scripts": {
        "copy:docs": "cp ../../CHANGELOG.md ../../LICENSE .",
        "clean:docs": "rm -f CHANGELOG.md LICENSE",
        "vscode:package": "pnpm run copy:docs && pnpm run package && pnpm exec vsce package --no-dependencies",
        "vscode:publish": "pnpm run copy:docs && pnpm exec vsce publish --no-dependencies",
        "vscode:prepublish": "pnpm run package",
        "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix --debug ",
        "format": "eslint src --fix",
        "test": "vscode-test"
    },
    "dependencies": {
        "@szmarczak/http-timer": "^5.0.1",
        "@trpc/client": "^11.0.0",
        "@vscode/test-cli": "^0.0.12",
        "@watchapi/parsers": "workspace:*",
        "flat": "^6.0.1",
        "got": "^14.6.6",
        "ts-morph": "^27.0.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "22.x",
        "@types/vscode": "^1.90.0",
        "@vitest/ui": "^2.0.0",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.2.1",
        "esbuild": "^0.27.1",
        "eslint": "^9.39.1",
        "npm-run-all": "^4.1.5",
        "ovsx": "^0.10.0",
        "ts-loader": "^9.5.4",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.1",
        "vitest": "^2.0.0",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1"
    }
}