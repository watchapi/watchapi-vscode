{
  "name": "@watchapi/cli",
  "version": "0.2.17",
  "description": "CLI tool for API monitoring and regression detection in CI/CD pipelines",
  "main": "dist/index.js",
  "bin": {
    "watchapi": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev:build": "tsup --watch",
    "dev": "node --loader ts-node/esm src/cli.ts",
    "dev:analyze": "node --loader ts-node/esm src/cli.ts analyze --root ./examples/nestjs",
    "dev:sync": "node --loader ts-node/esm src/cli.ts sync --root ./examples/nestjs",
    "start": "node dist/cli.js",
    "dev:run": "nodemon dist/cli.js",
    "check-types": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "api",
    "monitoring",
    "testing",
    "ci-cd",
    "regression"
  ],
  "author": "AresniyDev",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/api-monitoring.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/yourusername/api-monitoring#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@watchapi/parsers": "workspace:*",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "openapi-fetch": "^0.15.0",
    "ora": "^8.0.1",
    "ts-morph": "^27.0.2",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}